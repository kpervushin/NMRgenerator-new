  d23
  (center (p14:sp3 ph1):f2 (p22 ph1):f3 )
  d23
  (p21 ph2):f3

  20u cpd4:f4

  (p13:sp2 ph6):f2
  ;d28
  ;(p14:sp3 ph1):f2
  ;d28
  ;(p13:sp8 ph2):f2

  d0
  (p2 ph1):f1
  d0
  (center (p14:sp5 ph1):f2 (p22 ph1):f3 )
  d0
  (p2 ph1):f1
  d0
  (p14:sp3 ph1):f2
  DELTA
  (p14:sp5 ph1):f2
  4u

  ;(p13:sp2 ph10):f2
  ;d28
  ;(p14:sp3 ph1):f2
  ;d28
  (p13:sp8 ph11):f2

  4u do:f4

  if "l0 %2 == 1"
     {
     (p21 ph7):f3
     }
  else
     {
     (p21 ph8):f3
     }

