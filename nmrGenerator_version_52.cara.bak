<obj oid='154'>
<fld name='Type' type='String'>Pulse Program</fld>
<fld name='Author(s)' type='String'>K.Pervushin</fld>
<fld name='Datum' type='String'>27-1-2008</fld>
<fld name='Body' type='Memo'>
#define GRADIENT0 10u p20:gp0 200u
#define GRADIENT1 10u p21:gp1 200u
#define GRADIENT2 10u p21:gp2 200u
#define GRADIENT3 10u p21:gp3 200u
#define GRADIENT4 10u p21:gp4 200u
#define GRADIENT5 10u p25:gp5 200u ; crusher
#define GRADIENT6 10u p26:gp6 200u ; watergate
#define GRADIENT7 10u p27:gp7 200u ; crusher


#include &lt;Avance.incl&gt;
"p2=2*p1"


1 10u ze
  10u   
2 100u
3 100u 
4 100u
5 LOCKH_OFF
  10u pl9:f1
  d1 cw:f1
  10u do:f1
  20u LOCKH_ON
  GRADIENT0
  20u pl1:f1 
  1m
  
  (p1 ph1):f1
  GRADIENT1
  20u
  (p2 ph21):f1 
  GRADIENT2
  20u
  (p1 ph2):f1 ; return 1H to +z
  
  GRADIENT5
  d9 ; diffusion delay
  
  
  (p1 ph3):f1
  GRADIENT3
  20u
  (p2 ph21):f1 
  GRADIENT4
  20u
  
  (p1 ph20):f1
  GRADIENT7
  1m
  (p1 ph20):f1
  
  GRADIENT6 
  (p12:sp1 ph22):f1
  10u
  10u ;pl1:f1
  20u
  (p1*2 ph20:r):f1
  30u
  (p12:sp1 ph22):f1
  GRADIENT6 

  go=2 ph0
  100u wr #0 if #0 ipu21 zd
  lo to 5 times td1
  10u do:f1
  10u LOCKH_OFF 
exit

ph1=0 2
ph2=2 0 

ph3=0 0
ph20=0
ph21=1
ph22=2
ph23=3
ph0=0 0 

;##/($P[1],$PL[1])=&amp;SetPulse(f1,HP,90);
;##/$PL[2]=1;
;##/$P[6]=&amp;round(2*$LL_15N*exp(($PL[3]-$LPW_15N)/8.68589),0.000001);
;##/($P[4],$PL[2])=&amp;SetPulse(f2,HP,160);
;##/$P[4]=&amp;round($P[4],0.000001);
</fld>
