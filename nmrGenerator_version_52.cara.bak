<obj oid='240'>
<fld name='Macro Name' type='String'>atrosy_pfg_RDC</fld>
<fld name='Type' type='String'>Macro</fld>
<fld name='Author(s)' type='String'>K.Pervushin</fld>
<fld name='Datum' type='String'>20-12-2007</fld>
<fld name='Body' type='Memo'>exp_environment = {
d2= 1/(4.*couplings.J_HN.Hz),
d1 =1.,

["pulprog"]= "atrosy_pfg.pk",
o1 = spectrometer[now].proton_carrier_298k, 
o2p=spectrum.Carom.carrier, -- for 13C decoupling during t1
o3p=spectrum.N.carrier,
o4p=spectrum.D.carrier,
["xcpr"]="parmode 2D",

-- set hard pulses
p1 = spectrometer[now].calibration_pulse90_f1,
pl1= spectrometer[now].calibration_power_db_f1,
p3  = spectrometer[now].calibration_pulse90_f2,
pl2= spectrometer[now].calibration_power_db_f2,
p5 = spectrometer[now].calibration_pulse90_f3,
pl3 =spectrometer[now].calibration_power_db_f3,
pcpd4 = pulses.Dcpd90, -- D decoupling
pl4 = calc_power_set(pulses.Dcpd90,1.0,spectrometer[now].calibration_pulse90_f4,spectrometer[now].calibration_power_db_f4),
["cpdprg4"]= "waltz16", 


--set soft powers and pulses
p11 = pulses.watergate90, -- assume that watergate90 mks is ideal for 700
["spnam1"]= "Gauss_5_256.pk", -- integral factor 0.5
 sp1= calc_power_set(pulses.watergate90,0.5,spectrometer[now].calibration_pulse90_f1,spectrometer[now].calibration_power_db_f1),
 spoal1 =0.5, -- have to check what's that
 spoffs1=0,

--CO decoupling
["cpdprg2"]= "sed_p31.pk", -- sp15, p31
 p31 =pulses.COdec90, -- it is not that critical, assume 300 is ideal for 700
 ["spnam15"] ="Gauss_5_256.pk", -- integral factor 0.5
 sp15 = calc_power_set(pulses.COdec90,0.5,spectrometer[now].calibration_pulse90_f2,spectrometer[now].calibration_power_db_f2),
 spoal15 =0.5, -- have to check what's that
 cnst21 = spectrum.CO.carrier,

--set increment times
 ["1 in_010 "] = (0.5/(spectrum.N.sw*spectrometer[now].B_field*IndirectChemicalShiftReferences.N15)),
 
  --set number of sampled points
 ["1 td "]=(320), --H direction
 ["2 td "]=(1024), --H direction
 ["1 nd_010"]= 2 , --N direction 
 

 
 --gradients
gpnam1 ="SINE.100",
gpnam2 ="SINE.100",
gpnam3 ="SINE.100",
gpnam4 ="SINE.100",
gpnam9 ="SINE.100",
gpnam10 ="SINE.100",
gpz1 =25,
gpz2 =50,
gpz3 =80,
gpz4 =90,
p21 =700, --(Crusher in MIX)
p22 =800, --( HN/N transfer)
p23 =800, --(Z-Filter )
p24 =900, --(Z-Filter)

--gradients for echo-anti-echo selection of coherence
p29 =1800, --15N grad for echo-anti-echo
p30 =1800*IndirectChemicalShiftReferences.N15, --1H grad for echo-a-echo
gpz9 =-80,
gpz10=80,

--acqu modes
["1 FnMODE"]= "Echo-Antiecho",
["script1"]="trosy.m", --mathemtica description of TROSY

--processing
["1 mc2"]= "echo-antiecho",
["2 absf1"]=  12, 
["2 absf2"]=  5.5, 
["2 si"]=  2048,  --H dim
["2 stsi"]=  1024,
["1 si"]=  512, -- n dim 
["1 phc0"]=  90, -- n dim
["1 phc1"]=  -180, -- n dim 
["1 PH_mod"]=  "pk", -- n dim
["2 PH_mod"]=  "pk", -- n dim 
["1 REVERSE"]=  "FALSE", -- n dim 
["1 WDW"]="QSINE",
["2 WDW"]="QSINE",
["1 ssb"]=2,
["2 ssb"]=2,
["2 sr"]=0,  --referencing relative to absolute spectormeter calibration
["1 sf"]=spectrometer[now].B_field*IndirectChemicalShiftReferences.N15 -- N dim referencing relative to DSS
}

</fld>
