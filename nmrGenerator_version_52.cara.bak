["2 in_010 "] = (1/4/(spectrum.N.sw*spectrometer[now].B_field*IndirectChemicalShiftReferences.N15)),
["2 nd_010"]= 4 , --N direction 


  --set transfer delays
 d21=1/(2.*couplings.J_HN.Hz), --1/(2J(NH)
 d23= 0.8*1/(4.*couplings.J_CaN.Hz),
 d26= 1/(4.*couplings.J_HN.Hz),
 d28= 0.8*1/(4.*couplings.J_CaCO.Hz),
 
 ["1 td "]=128,
 ["2 td"]= 1*(10.9e-3)/((1/2/(spectrum.N.sw*spectrometer[now].B_field*IndirectChemicalShiftReferences.N15))),
 
 --set gradients
["gpnam1"]="SINE.100",
["gpnam2"]="SINE.100",
["gpnam3"]="SINE.100",
["gpnam4"]="SINE.100",
