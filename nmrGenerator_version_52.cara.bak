exp_environment = {

["pulprog"]= "hsqc15N_CPMG_RD.pk", 
o1 = spectrometer[now].proton_carrier_298k, 
o2p=spectrum.Carom.carrier, 
o3p=spectrum.N.carrier,
o4p=spectrum.D.carrier,
["xcpr"]="parmode 3D",
d1=2.,
