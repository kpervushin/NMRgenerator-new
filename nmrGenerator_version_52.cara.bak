<obj oid='202'>
<fld name='Macro Name' type='String'>4D noesy time shared</fld>
<fld name='Type' type='String'>Macro</fld>
<fld name='Author(s)' type='String'>Daiwen Yang</fld>
<fld name='Datum' type='String'>23-9-2008</fld>
<fld name='Body' type='Memo'>exp_environment = {

d1 =1.,
d9 =0.12, -- (s) mixing time
ns=4,

["pulprog"]= "noesy_nc4dshare1.pk",

-- set carrier 
o1 = spectrometer[now].proton_carrier_298k, 
o2p=spectrum.Ca.carrier, 
o3p=spectrum.N.carrier,
o4p=spectrum.D.carrier, 
["xcpr"]="parmode 3D",


--set hard powers and pulses for f1-f4
p1 = spectrometer[now].calibration_pulse90_f1,
pl1= spectrometer[now].calibration_power_db_f1,
p5  = spectrometer[now].calibration_pulse90_f2,
pl2= spectrometer[now].calibration_power_db_f2,
p3 = spectrometer[now].calibration_pulse90_f3,
pl3 =spectrometer[now].calibration_power_db_f3,
pl19 =60, --water presat (when used) 

p11=pulses.watergate90,
["spnam1"] ="Gauss_5_256.pk", -- integral factor 0.5
 sp1 = calc_power_set(pulses.watergate90,0.5,spectrometer[now].calibration_pulse90_f1,spectrometer[now].calibration_power_db_f1),
pl11 = calc_power_set(pulses.watergate90,0.5,spectrometer[now].calibration_pulse90_f1,spectrometer[now].calibration_power_db_f1),


pcpd2 = 1300.,
sp15 = calc_power_set(25000./3500.*10.,1.0,spectrometer[now].calibration_pulse90_f2,spectrometer[now].calibration_power_db_f2),
["cpdprg2"]="p5m4sp180",
["spnam15"] ="SChirp_40k_1300_20",
["spoffs15"]=0.0,

pcpd3 = 250.0 ,--pulses.Ncpd90,
pl13 = calc_power_set(250.0,1.0,spectrometer[now].calibration_pulse90_f3,spectrometer[now].calibration_power_db_f3),
["cpdprg3"]="garp4",

--CO decoupling
 p30 =100., -- it is not that critical, assume 300 is ideal for 700
 ["spnam30"] ="Sed.200", -- integral factor 0.46
 pl22 = calc_power_set(100./2.,0.46,spectrometer[now].calibration_pulse90_f2,spectrometer[now].calibration_power_db_f2),
 sp30 = calc_power_set(100./2.,0.46,spectrometer[now].calibration_pulse90_f2,spectrometer[now].calibration_power_db_f2),
 spoal30 =0.5, -- have to check what's that
 ["spoffs30"]=(spectrum.CO.carrier-spectrum.Ca.carrier)*spectrometer[now].B_field*IndirectChemicalShiftReferences.C13,
["cpdprg4"]="mlevsp180.p30",


p12 = 400.,
sp2 = calc_power_set(25000./8000.*10.,1.0,spectrometer[now].calibration_pulse90_f2,spectrometer[now].calibration_power_db_f2),
pl0 = calc_power_set(25000./8000.*10.,1.0,spectrometer[now].calibration_pulse90_f2,spectrometer[now].calibration_power_db_f2),
["spnam2"] ="CaWurst.400_55k",
["spoffs2"]=0.0,

p14 = 600.,
sp3 = calc_power_set(25000./7000.*10.,1.0,spectrometer[now].calibration_pulse90_f2,spectrometer[now].calibration_power_db_f2),
["spnam3"] ="CaWurst.600_55k",
