  (center (p14:sp3 ph1):f2 (p22 ph1):f3 )
  d27
  (p14:sp7 ph1):f2
  4u
  (p13:sp8 ph1):f2

  (p21 ph1):f3
  d30
  (p14:sp7 ph1):f2
  d30
  (center (p14:sp3 ph1):f2 (p22 ph8):f3 )
  d10
  (p14:sp7 ph1):f2
  d29
