function t.drawspec(imax, specnumlist, colors, peaklst, colorpeaklst, cp)

-- Initialize lots of stuff 
  w=gui.createWidget()
  w:setCaption("mulitplespectrumview2: Viewer for multiple NMR Spectra")
  w:setBgColor(t.colorbgd)
  w:show()
  w:getData().spec={}
  w:getData().color={}
  w:getData().buf={}
  w:getData().c={}

-- Define refresh buffer function

  function refresh(widget)
    for ii=1,imax do
      widget:getData().buf[ii]=w:getData().spec[ii]:getPlanePpm(1,2,w:getData().ppmmax1,w:getData().ppmmin1,w:getData().ppmmax2,w:getData().ppmmin2)
      widget:getData().c[ii]=dlg.createContour(w:getData().buf[ii])
      widget:getData().c[ii]:setPosColor(w:getData().color[ii])
      widget:getData().c[ii]:setNegColor(w:getData().color[ii])
      widget:getData().c[ii]:setLineWidth(cp.linewidth[ii])
      widget:getData().c[ii]:setParams(cp.factor[ii],cp.baseline[ii])
    end
    widget:update()
  end -- refresh function

-- Get colors &amp; spectra
  for ii=1,imax do
    w:getData(colors[ii]).color[ii]=colors[ii]  
    w:getData().spec[ii]=cara:getProject( t.ProjectName ):getSpectrum(specnumlist[ii])
  end

-- Determine max, min ppm
  w:getData().ppmmax1,w:getData().ppmmin1=w:getData().spec[1]:getPpmRange(1)
  w:getData().ppmmin2,w:getData().ppmmax2=w:getData().spec[1]:getPpmRange(2)

-- Write Buffers
--[[ -- replaced with refresh(w) below
  for ii=1,imax do
    w:getData().buf[ii]=w:getData().spec[ii]:getPlanePpm(1,2,w:getData().ppmmax1,w:getData().ppmmin1,w:getData().ppmmax2,w:getData().ppmmin2)
    w:getData().c[ii]=dlg.createContour(w:getData().buf[ii])
    w:getData().c[ii]:setPosColor(w:getData().color[ii])
    w:getData().c[ii]:setNegColor(w:getData().color[ii])
    w:getData().c[ii]:setLineWidth(cp.linewidth[ii])
    w:getData().c[ii]:setParams(cp.factor[ii],cp.baseline[ii])
  end
]]

  refresh(w)

-- Draw and Redraw
  w:setCallback(gui.event.Paint,
    function (self, p)
       local width,height = self:getSize()
       for ii=1,imax do
         p:drawContour( self:getData().c[ii],0,0,width,height)
       end
       if peaklst&gt;0 then
         t.out=t.drawpklst(p,self:getData().c[1],colorpeaklst,peaklst, width, height)
       end
       -- draw Cursor HERE
       t.drawcursor(p, self:getData().c[1], self:getData().XcursorPos, self:getData().YcursorPos, width, height)
    end ) -- end Draw and Redraw Callback

  w:setCallback(gui.event.MousePressed,
    function (self, x,y,left,mid,right,shift,ctrl,alt)
       local width,height = self:getSize()
       if ctrl==true and shift==true then
         self:getData().ppmmin1=self:getData().c[1].toPpm(self:getData().c[1],1,0,width,x)
         self:getData().ppmmin2=self:getData().c[1].toPpm(self:getData().c[1],2,0,height,y)
       elseif ctrl==true and shift==false then
         -- report
--       print("Here is Ctrl mouse click button")
         self:getData().CtrlClickStartX=self:getData().c[1].toPpm(self:getData().c[1],1,0,width,x)
         self:getData().CtrlClickStartY=self:getData().c[1].toPpm(self:getData().c[1],2,0,height,y)
         print("ctrlclick X="..self:getData().CtrlClickStartX.."ctrlclick Y="..self:getData().CtrlClickStartY )
       elseif ctrl==false and shift==false then
         self:getData().XcursorPos=self:getData().c[1].toPpm(self:getData().c[1],1,0,width,x)
         self:getData().YcursorPos=self:getData().c[1].toPpm(self:getData().c[1],2,0,height,y)
         print( "x="..string.format("%7.3f",self:getData().XcursorPos).." / y="..string.format("%7.3f",self:getData().YcursorPos) )
         self:update()
       end
    end ) -- end MousePressed Callback

  w:setCallback(gui.event.MouseReleased,
    function (self, x,y,left,mid,right,shift,ctrl,alt)
       local width,height = self:getSize()
       if ctrl==true and shift==true then
         self:getData().ppmmax1=self:getData().c[1].toPpm(self:getData().c[1],1,0,width,x)
         self:getData().ppmmax2=self:getData().c[1].toPpm(self:getData().c[1],2,0,height,y)
  
         if self:getData().ppmmax1&lt;self:getData().ppmmin1 then
           self:getData().temp=self:getData().ppmmax1
           self:getData().ppmmax1=self:getData().ppmmin1
           self:getData().ppmmin1=self:getData().temp
         end 
         if self:getData().ppmmax2&gt;self:getData().ppmmin2 then
           self:getData().temp=self:getData().ppmmax2
           self:getData().ppmmax2=self:getData().ppmmin2
           self:getData().ppmmin2=self:getData().temp
         end
         refresh(self)

       elseif ctrl==true and shift==false and self:getData().CtrlClickStartX then
--         print("OK I saw a CtrlClickRelease")
         local CtrlClickStopX=self:getData().c[1].toPpm(self:getData().c[1],1,0,width,x)
         local CtrlClickStopY=self:getData().c[1].toPpm(self:getData().c[1],2,0,height,y)
         -- determine DeltaX and DeltaY
         local DeltaX = CtrlClickStopX - self:getData().CtrlClickStartX
         local DeltaY = CtrlClickStopY - self:getData().CtrlClickStartY
         -- determine new ppmmin1 etc.
         local NewPpmMin1 = self:getData().ppmmin1 - DeltaX
         local NewPpmMin2 = self:getData().ppmmin2 - DeltaY
         local NewPpmMax1 = self:getData().ppmmax1 - DeltaX
         local NewPpmMax2 = self:getData().ppmmax2 - DeltaY
         -- check whether new ppmmin is outside of PpmRange
         -- report
         -- print("ctrl click release: DeltaX="..DeltaX.." DeltaY="..DeltaY.." NewPpmMin1="..NewPpmMin1.." NewPpmMax1="..NewPpmMax1.." NewPpmMin2="..NewPpmMin2.." NewPpmMax2="..NewPpmMax2 )
         self:getData().ppmmax1=NewPpmMax1
         self:getData().ppmmin1=NewPpmMin1
         self:getData().ppmmax2=NewPpmMax2
         self:getData().ppmmin2=NewPpmMin2
  
         self:update()

         refresh(self)

         self:getData().CtrlClickStartX = nil
         self:getData().CtrlClickStartY = nil
       end
    end ) -- end MouseReleased Callback


  w:setCallback(gui.event.KeyReleased,
    function (self, keyCode,text,shift,ctrl,alt)
       local width,height = self:getSize()
  
--   Define keyCode responses

       if keyCode == 4112 then -- (Home Key) resets full ppm range
         self:getData().ppmmax1,self:getData().ppmmin1=self:getData().spec[1]:getPpmRange(1)
         self:getData().ppmmin2,self:getData().ppmmax2=self:getData().spec[1]:getPpmRange(2)
         refresh(self)
--     Navigation in Spectrum Up, Down, Left, Right
       elseif keyCode == 4118 and ctrl==false then -- (PageUp) shifts ppm range up by one half window range
         PpmPresentRange2 = self:getData().ppmmax2 - self:getData().ppmmin2
         self:getData().ppmmin2 = self:getData().ppmmin2 + PpmPresentRange2/2
         self:getData().ppmmax2 = self:getData().ppmmax2 + PpmPresentRange2/2
         refresh(self)
       elseif keyCode == 4119 and ctrl==false then -- (PageDown) shifts ppm range down by one half window range
         PpmPresentRange2 = self:getData().ppmmax2 - self:getData().ppmmin2
         self:getData().ppmmin2 = self:getData().ppmmin2 - PpmPresentRange2/2
         self:getData().ppmmax2 = self:getData().ppmmax2 - PpmPresentRange2/2
         refresh(self)
       elseif keyCode == 4118 and ctrl== true then -- (Ctrl-PageUp) shifts ppm range left by one half window range
         PpmPresentRange1 = self:getData().ppmmax1 - self:getData().ppmmin1
         self:getData().ppmmin1 = self:getData().ppmmin1 + PpmPresentRange1/2
         self:getData().ppmmax1 = self:getData().ppmmax1 + PpmPresentRange1/2
         refresh(self)
       elseif keyCode == 4119 and ctrl== true then -- (Ctrl-PageDown) shifts ppm range right by one half window range
         PpmPresentRange1 = self:getData().ppmmax1 - self:getData().ppmmin1
         self:getData().ppmmin1 = self:getData().ppmmin1 - PpmPresentRange1/2
         self:getData().ppmmax1 = self:getData().ppmmax1 - PpmPresentRange1/2
         refresh(self)
--     Expand and Contract Region: Shift-Ctrl Up,Down,Left,and Right
       elseif keyCode == 4115 and ctrl == true and shift == true then -- (Shift-Ctrl-UpArrow) expand region vertically 2X
         PpmPresentRange2 = self:getData().ppmmax2 - self:getData().ppmmin2
         PpmPresentCenter2 = ( self:getData().ppmmax2 + self:getData().ppmmin2 ) / 2
         self:getData().ppmmin2 = PpmPresentCenter2 - PpmPresentRange2/4
         self:getData().ppmmax2 = PpmPresentCenter2 + PpmPresentRange2/4
         refresh(self)
       elseif keyCode == 4117 and ctrl == true and shift == true then -- (Shift-Ctrl-DownArrow) contract region vertically 2X
         PpmPresentRange2 = self:getData().ppmmax2 - self:getData().ppmmin2
         PpmPresentCenter2 = ( self:getData().ppmmax2 + self:getData().ppmmin2 ) / 2
         self:getData().ppmmin2 = PpmPresentCenter2 - PpmPresentRange2
         self:getData().ppmmax2 = PpmPresentCenter2 + PpmPresentRange2
         refresh(self)
       elseif keyCode == 4116 and ctrl == true and shift == true then -- (Shift-Ctrl-RightArrow) expand region vertically 2X
         PpmPresentRange1 = self:getData().ppmmax1 - self:getData().ppmmin1
         PpmPresentCenter1 = ( self:getData().ppmmax1 + self:getData().ppmmin1 ) / 2
         self:getData().ppmmin1 = PpmPresentCenter1 - PpmPresentRange1/4
         self:getData().ppmmax1 = PpmPresentCenter1 + PpmPresentRange1/4
         refresh(self)
       elseif keyCode == 4114 and ctrl == true and shift == true then -- (Shift-Ctrl-LeftArrow) contract region vertically 2X
         PpmPresentRange1 = self:getData().ppmmax1 - self:getData().ppmmin1
         PpmPresentCenter1 = ( self:getData().ppmmax1 + self:getData().ppmmin1 ) / 2
         self:getData().ppmmin1 = PpmPresentCenter1 - PpmPresentRange1
         self:getData().ppmmax1 = PpmPresentCenter1 + PpmPresentRange1
         refresh(self)

       end

    end ) -- end KeyRelease Callback

  w:update()
end -- function t.drawspec
