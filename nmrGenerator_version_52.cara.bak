  d16
  DELTA1
  (center (p2 ph2) (p22 ph1):f3 )
  DELTA1
  p16:gp1
  d16
  (p1 ph2):f1 

  (p21 ph1):f3
  p16:gp2
  d16
  DELTA2
  (center (p14:sp3 ph1):f2 (p22 ph1):f3 )
  DELTA2
  p16:gp2
  d16
  (p21 ph2):f3

  (p13:sp2 ph6):f2
  4u
  (p14:sp5 ph1):f2
  p16:gp1
  d16
  DELTA3
  (p14:sp3 ph1):f2
  4u
  (p14:sp5 ph1):f2
  DELTA3
  p16:gp1
  d16
  (p13:sp8 ph2):f2

  4u cpd4:f4
  30u fq=cnst22(bf ppm):f2

  (p13:sp2 ph7):f2
  d0
  (p2 ph1):f1 
  d0
  (center (p14:sp7 ph1):f2 (p22 ph1):f3 )
  d0
  (p2 ph1):f1 
  d0
  (p14:sp3 ph1):f2
  DELTA
  (p14:sp7 ph1):f2
  4u do:f4
