<fld name='Body' type='Memo'>;dipsitretf3gp3d
;avance-version (05/10/20)
;DIPSI2-TROSY
;3D sequence with
;   homonuclear Hartman-Hahn transfer using DIPSI2 sequence
;      for mixing
;   H-1/X correlation via TROSY
;      using sensitivity improvement
;phase sensitive (t1)
;phase sensitive using Echo/Antiecho-TPPI gradient selection (t2)
;using f3 - channel
;allowing for presaturation during relaxation delay in cases
;   of radiation damping
;(use parameterset DIPSITRETF3GP3D)
;
;G. Zhu, X.M. Kong &amp; K.H. Sze, J. Biomol. NMR 13, 77-81 (1999)
;M. Czisch &amp; R. Boelens, J. Magn. Reson. 134, 158-160 (1998)
;K. Pervushin, G. Wider &amp; K. Wuethrich, J. Biomol. NMR 12,
;   345-348 (1998)
;A. Meissner, T. Schulte-Herbrueggen, J. Briand &amp; O.W. Sorensen, Mol. Phys. 96,
;   1137-1142 (1998)
;J. Weigelt, J. Am. Chem. Soc. 120, 10778-10779 (1998)
;M. Rance, J.P. Loria &amp; A.G. Palmer III, J. Magn. Reson. 136, 91-101 (1999)
;
;$CLASS=HighRes
;$DIM=3D
;$TYPE=
;$SUBTYPE=
;$COMMENT=


#include &lt;Avance.incl&gt;
#include &lt;Grad.incl&gt;
#include &lt;Delay.incl&gt;


"p2=p1*2"
"p22=p21*2"
"d0=6u"
"d10=6u"
"d11=30m"
"d12=20u"
;"d26=1s/(cnst4*4)"


"DELTA1=d26-p16-d16-4u"
"DELTA2=d26-p1-p16-d16-4u"
"DELTA3=p19+d16+8u"

#   ifdef LABEL_CN
"DELTA=d10*2+p14+p21*4/3.1416+12u"
"DELTA4=d0*2+larger(p14,p22)+4u"
#   else
"DELTA=d10*2+p21*4/3.1416+12u"
"DELTA4=d0*2+p22+4u"
#   endif /*LABEL_CN*/


"FACTOR1=(d9/(p6*115.112))/2+0.5"
"l1=FACTOR1*2"

"l0=1"


aqseq 321


1 ze 
2 d11
3 d12 pl9:f1
  d1 cw:f1 ph29
  4u do:f1
  4u pl1:f1
  50u UNBLKGRAD
  (p1 ph1)
  DELTA4
  (p2 ph2)
  d0 gron0
  2u groff

#   ifdef LABEL_CN
  (center (p14:sp3 ph1):f2 (p22 ph1):f3 )
#   else
  (p22 ph1):f3
#   endif /*LABEL_CN*/

  d0 gron0*-1
  2u groff

  (p1 ph9)
  10u pl10:f1
						;begin DIPSI2
7 p6*3.556 ph23
  p6*4.556 ph21
  p6*3.222 ph23
  p6*3.167 ph21
  p6*0.333 ph23
  p6*2.722 ph21
  p6*4.167 ph23
  p6*2.944 ph21
  p6*4.111 ph23
  
  p6*3.556 ph21
  p6*4.556 ph23
  p6*3.222 ph21
  p6*3.167 ph23
  p6*0.333 ph21
  p6*2.722 ph23
  p6*4.167 ph21
  p6*2.944 ph23
  p6*4.111 ph21

  p6*3.556 ph21
  p6*4.556 ph23
  p6*3.222 ph21
  p6*3.167 ph23
  p6*0.333 ph21
  p6*2.722 ph23
  p6*4.167 ph21
  p6*2.944 ph23
  p6*4.111 ph21

  p6*3.556 ph23
  p6*4.556 ph21
  p6*3.222 ph23
  p6*3.167 ph21
  p6*0.333 ph23
  p6*2.722 ph21
  p6*4.167 ph23
  p6*2.944 ph21
  p6*4.111 ph23
  lo to 7 times l1
						;end DIPSI2

  10u pl1:f1
  (p1 ph10)

  4u
  p16:gp1
  d16 pl1:f1
  DELTA1
  (center (p2 ph3) (p22 ph1):f3 )
  4u
  DELTA1
  p16:gp1
  d16
  (p1 ph4) 

  if "l0 %2 == 1"
     {
     (p21 ph5):f3
     }
  else
     {
     (p21 ph6):f3
     }

  d10 gron0
  6u groff

#   ifdef LABEL_CN
  (p14:sp3 ph1):f2
#   else
#   endif /*LABEL_CN*/

  d10 gron0*-1
  6u groff

  p19:gp2*EA
  d16
  (p22 ph1):f3

  DELTA

  p19:gp2*-1*EA
  d16

  (p1 ph7)
  4u
  p16:gp3
  d16
  DELTA2 pl1:f1
  (center (p2 ph1) (p22 ph1):f3 )
  4u
  DELTA1
  p16:gp3
  d16
  (center (p1 ph1) (p21 ph2):f3 )

  4u
  p16:gp4
  d16
  DELTA1
  (center (p2 ph1) (p22 ph1):f3 )
  DELTA1
  p16:gp4
  d16
  4u
  (p21 ph8):f3

  DELTA3
  (p2 ph1)
  4u
  p19:gp5
  d16
  4u BLKGRAD

  go=2 ph31
  d11 mc #0 to 2 
     F1PH(rd10 &amp; rp5 &amp; rp6 &amp; rp31 &amp; dp3 &amp; dp4 &amp; dp9 &amp; dp10 &amp; dp21 &amp; dp23, id0) 
     F2EA(igrad EA &amp; ip7*2 &amp; ip8*2 &amp; iu0, id10 &amp; ip5*2 &amp; ip6*2 &amp; ip31*2)
exit 
  

ph1=0
ph2=1
ph3=1 1 1 1 3 3 3 3
ph4=3 3 3 3 1 1 1 1
ph5=1 3 2 0
ph6=3 1 2 0
ph7=1
ph8=0
ph9=2 2 2 2 0 0 0 0
ph10=0 0 0 0 2 2 2 2
ph21=1 1 1 1 3 3 3 3
ph23=3 3 3 3 1 1 1 1
ph29=0
ph31=0 2 3 1 2 0 1 3


;pl1 : f1 channel - power level for pulse (default)
;pl3 : f3 channel - power level for pulse (default)
;pl9 : f1 channel - power level for presaturation
;pl10: f1 channel - power level for TOCSY-spinlock
;sp3: f2 channel - shaped pulse 180 degree (adiabatic)
;p1 : f1 channel -  90 degree high power pulse
;p2 : f1 channel - 180 degree high power pulse
;p6 : f1 channel -  90 degree low power pulse
;p14: f2 channel - 180 degree shaped pulse for inversion (adiabatic)
;p16: homospoil/gradient pulse                         [1 msec]
;p19: gradient pulse 2                                 [500 usec]
;p21: f3 channel -  90 degree high power pulse
;p22: f3 channel - 180 degree high power pulse
;d0 : incremented delay (F1 in 3D)                     [6 usec]
;d1 : relaxation delay; 1-5 * T1
;d9 : TOCSY mixing time
;d10: incremented delay (F2 in 3D)                     [6 usec]
;d11: delay for disk I/O                               [30 msec]
;d12: delay for power switching                        [20 usec]
;d16: delay for homospoil/gradient recovery
;d26 : 1/(4J)YH
;cnst4: = J(YH)
;l1: loop for DIPSI cycle: ((p6*115.112) * l1) = mixing time
;in0: 1/(2 * SW(H)) = DW(H)
;nd0: 2
;in10: 1/(2 * SW(X)) = DW(X)
;nd10: 2
;NS: 8 * n
;DS: 16
;td1: number of experiments
;td2: number of experiments in F2
;FnMODE: States-TPPI (or TPPI) in F1
;FnMODE: echo-antiecho in F2


;use gradient ratio:    gp 0 : gp 1 : gp 2 : gp 3 : gp 4 : gp 5
;                          3 :   30 :   80 :   45 :   50 : 16.2

;for z-only gradients:
;gpz0: 3%
;gpz1: 30%
;gpz2: 80%
;gpz3: 45%
;gpz4: 50%
;gpz5: 16.2%

;use gradient files:
;gpnam1: SINE.100
;gpnam2: SINE.50
;gpnam3: SINE.100
;gpnam4: SINE.100
;gpnam5: SINE.50


;set pl9 to 120dB when presaturation is not required
;   use 75 - 80dB to reduce radiation damping


                                          ;preprocessor-flags-start
;LABEL_CN: for C-13 and N-15 labeled samples start experiment with 
;             option -DLABEL_CN (eda: ZGOPTNS)
                                          ;preprocessor-flags-end



;$Id: dipsitretf3gp3d,v 1.10.8.1 2005/11/10 13:18:56 ber Exp $
</fld>
