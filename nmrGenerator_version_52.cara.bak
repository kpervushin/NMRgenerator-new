<obj oid='114'>
<fld name='Type' type='String'>Pulse Program</fld>
<fld name='Author(s)' type='String'>K.Pervushin</fld>
<fld name='Datum' type='String'>4-1-2008</fld>
<fld name='Body' type='Memo'>;TOCSY_HHC.eth  15,13C-resolved [H,H]-TOCSY


;d1:  relaxation delay
;d2    : 1/4J (CH)
;d9    : mixing time

;in0: increment for 1H evolution (=dw)
;in10: increment for 15N evolution

;p1    : 90 degree hard pulse 1H
;p2    : 180 degree 1H
;p5    : 90 degree hard pulse 15N
;p6    : 180 degree 15N
;p3    : 90 degree hard pulse 13C
;pl1   : power for 1H 
;pl2   : power for 15N
;pl3   : power for 13C hard pulse
;pl13  : power for 13C decoupling
;cpd3  : decoupling according to sequence by cpdprg3
;pcpd3 : 90 degree pulse for decoupling (pl13)
;sp15  : power for CO-decopling
;p20   : 1000u (Gradient before acquisition)
;gpz0  : 60 %
;p21   : 1100u (Gradient in mixing time)
;gpz1  : 65 %
;p22   : 800u (Gradient in first INEPT)
;gpz2  : 25 %
;p23   : 1000u (Gradient for 1. z-filter)
;gpz3  : 70 %
;p24   : 900u (Gradient for 2. z-filter)
;gpz4  : -55 %
;p25   : 700u (Gradient in second INEPT)
;gpz5  : 15 %


"p2=2*p1"
"p6=2*p5"
"p4=2*p3"
"d0=in0/2-p1*2/3.14159"
"d10=in10/2-p5*2/3.14159-14u-p3-p1"
"d8=d9-(p21+210u)"
"d2=2.5m"
"d14=d2-p22-210u"
"d15=0.9m"
"d16=d14-d15-p3*4-3u"


"l3=td1/2"
"l13=td2/2"

"spoff15=bf2*(cnst21/1000000)-o2"


#define GRADIENT0  10u p20:gp0 200u
#define GRADIENT1  10u p21:gp1 200u
#define GRADIENT2  10u p22:gp2 200u
#define GRADIENT3  10u p23:gp3 200u
#define GRADIENT4  10u p24:gp4 200u
#define GRADIENT5  10u p22:gp5 200u
#define GRADIENT6  10u p26:gp6 200u



#include &lt;Avance.incl&gt;

aqseq 312

"FACTOR1=(d9/(p6*115.112))/2+0.5"
"l1=FACTOR1*2"


1 100u ze

2 10u do:f2
  10u do:f3 
  20u pl19:f1
  10u LOCKH_OFF
  d1 cw:f1
  10u do:f1
  20u pl1:f1 pl2:f2 pl3:f3 
  20u LOCKH_ON
  GRADIENT0
  10u
 

;--------------1H evolution
(p1 ph1 d0 d0 p1 ph2):f1 (d0 p4 ph20):f2 (d0 p6 ph20):f3

;--------------mixing time
  GRADIENT1
    10u pl10:f1
						;begin DIPSI2
7 p12*3.556 ph15
  p12*4.556 ph17
  p12*3.222 ph15
  p12*3.167 ph17
  p12*0.333 ph15
  p12*2.722 ph17
  p12*4.167 ph15
  p12*2.944 ph17
  p12*4.111 ph15
  
  p12*3.556 ph17
  p12*4.556 ph15
  p12*3.222 ph17
  p12*3.167 ph15
  p12*0.333 ph17
  p12*2.722 ph15
  p12*4.167 ph17
  p12*2.944 ph15
  p12*4.111 ph17

  p12*3.556 ph17
  p12*4.556 ph15
  p12*3.222 ph17
  p12*3.167 ph15
  p12*0.333 ph17
  p12*2.722 ph15
  p12*4.167 ph17
  p12*2.944 ph15
  p12*4.111 ph17

  p12*3.556 ph15
  p12*4.556 ph17
  p12*3.222 ph15
  p12*3.167 ph17
  p12*0.333 ph15
  p12*2.722 ph17
  p12*4.167 ph15
  p12*2.944 ph17
  p12*4.111 ph15

  lo to 7 times l1
						;end DIPSI2

  10u pl1:f1
 
  GRADIENT0
  (p1 ph20):f1
;--------------first INEPT
   GRADIENT2
  d14
  (refalign (p2 ph20):f1 center (p6 ph20):f3)
  d15 
  (p4 ph21):f2
  GRADIENT2
  d16 
  (p1*300 ph20):f1
  (p1 ph21):f1
  GRADIENT3
;--------------13C,15N evolution
 "d17=d10*0.8012"
  4u
 "d18=d10*0.1988"
  4u
  10u cpds5:f2
10u
 (p5 ph3):f3
   d17 
5u do:f2
   10u pl2:f2  
  (p3 ph3):f2
  10u cpds5:f2
   d18

  (p2 ph20):f1

   d18
   4u do:f2 
   10u pl2:f2  
  (p3 ph20):f2
  10u cpds5:f2
   d17
  (p5 ph20):f3
  5u do:f2
  GRADIENT3
  (p1 ph21):f1
  ;(p1*200 ph20):f1
  ;--------------second INEPT

   GRADIENT2
  d14
  (refalign (p2 ph20):f1 center (p6 ph20):f3)
  d15 
  (p3 ph20 1.5u p4 ph21 1.5u p3 ph20):f2
  GRADIENT2
  d16 pl13:f3 pl12:f2
  GRADIENT6
   (p11:sp2 ph26:r):f1
   20u ;pl1:f1  
   (p2 ph20):f1 
   10u 
   (p11:sp2 ph26:r):f1  
   10u
  GRADIENT6
  10u LOCKH_OFF
  go=2 ph0 cpd3:f3 cpd2:f2
   1m do:f2 do:f3 mc #0 to 2
  F1PH(ip1,id0)
  F2PH(rd0 &amp; rp1 &amp; ip3,id10)

  
  20u LOCKH_OFF
exit


ph1=1 1 3 3 
ph2=1 1 1 1 		              
ph3=2 0
ph4=1 
ph10=1
ph11=0 0 0 0
ph12=1 1 1 1

ph15=3
ph17=1

ph20=0
ph21=1
ph22=2
ph23=3
ph26=0

ph0=0 2 2 0 

;##/($P[1],$PL[1])= &amp;SetPulse(f1,HP,90);
;##/($P[3],$PL[2])= &amp;SetPulse(f2,HP,90);
;##/($P[5],$PL[3])= &amp;SetPulse(f3,HP,90);
;##/$P[18]= &amp;SetShape(f3,1,&amp;round(0.032/$BF3,0.00001),180,174);
;##/($PCPD[3],$PL[13])= &amp;SetPulse(f3,0.000085,90);
;##\open(FP_f1,"&gt;/u/exp/stan/nmr/lists/f1/$FQ3LIST")||die "can not open $f1_li";
;##\print FP_f1 "O $BF3\n";
;##\printf FP_f1 "%.1f\n",$BF3*77;
;##\printf FP_f1 "%.1f\n",$BF3*$CNST[30];
;##\close FP_f1;
</fld>
