<fld name='Body' type='Memo'>;trhncagp3d
;avance-version (05/10/28)
;TROSY-HNCA
;3D sequence with
;   inverse correlation for triple resonance
;      via TROSY and HMQC steps
;
;      F1(H) -&gt; F3(N,t1) -&gt; F2(Ca,HMQC,t2) -&gt; F3(N,t1) -&gt; F1(H,t3)
;
;on/off resonance Ca and C=O pulses using shaped pulse
;phase sensitive using Echo/Antiecho (t1)
;phase sensitive (t2)
;using constant time in t1
;(use parameterset TRHNCAGP3D)
;
;M. Salzmann, K. Pervushin, G. Wider, H. Senn &amp; K. Wuethrich, Proc.
;   Natl. Acad. Sci. USA 95, 13585-13590 (1998)
;
;$CLASS=HighRes
;$DIM=3D
;$TYPE=
;$SUBTYPE=
;$COMMENT=


prosol relations=&lt;triple&gt;


#include &lt;Avance.incl&gt;
#include &lt;Grad.incl&gt;
#include &lt;Delay.incl&gt;


"p2=p1*2"
"p22=p21*2"
"d0=3u"
"d10=3u"
"d11=30m"
"d12=20u"


"d23=12m"
"d26=2.3m"


"in20=in0"
"in28=in0"
"in30=in10"

"d20=d23+p13+d10*2+p14/2"
"d28=d23+p13+d10*2+p14/2-p11-p1-8u"
"d30=d23-p14"


"DELTA1=d26-p11-p16-d16-8u"
"DELTA2=d26-p16-d16"


"l0=1"


"spoff2=0"
"spoff3=0"
"spoff5=bf2*(cnst21/1000000)-o2"
"spoff8=0"


aqseq 312


1 d11 ze
2 d11
3 d1 pl1:f1
  50u UNBLKGRAD

  p1 ph1
  4u pl0:f1
  (p11:sp1 ph3:r):f1
  4u
  p16:gp1
  d16 pl1:f1
  DELTA1
  (center (p2 ph2) (p22 ph1):f3 )
  DELTA1
  p16:gp1
  d16 pl0:f1
  (p11:sp1 ph4:r):f1
  4u
  4u pl1:f1
  (p1 ph4):f1 

  if "l0 %2 == 1"
     {
     (p21 ph5):f3
     }
  else
     {
     (p21 ph6):f3
     }

  d10
  d20
  (center (p14:sp3 ph1):f2 (p22 ph1):f3 )
  d30
  (p14:sp5 ph1):f2 
  d0

  (p13:sp2 ph1):f2
  d10
  d10
  (p14:sp5 ph1):f2 
  d10
  d10
  (p13:sp8 ph7):f2

  d0
  (p14:sp5 ph1):f2
  d30
  (center (p14:sp3 ph1):f2 (p22 ph1):f3 )
  d10

  d28 pl0:f1
  (p11:sp1 ph1:r):f1
  4u
  4u pl1:f1

  (p1 ph8)
  p16:gp2
  d16
  DELTA2
  (center (p2 ph1) (p22 ph1):f3 )
  DELTA2
  p16:gp2
  d16
  (center (p1 ph1) (p21 ph8):f3 )
  p16:gp3
  d16
  DELTA1 pl0:f1
  (p11:sp1 ph3:r):f1
  4u
  4u pl1:f1
  (center (p2 ph1) (p22 ph1):f3 )
  4u pl0:f1
  (p11:sp1 ph3:r):f1
  DELTA1
  p16:gp3
  d16 pl1:f1
  4u BLKGRAD
  (p21 ph1):f3
  go=2 ph31
  d11 mc #0 to 2 
     F1EA(ip8*2 &amp; iu0, id0 &amp; dd20 &amp; dd28) 
     F2PH(rd0 &amp; rd20 &amp; rd28 &amp; ip7, id10 &amp; dd30)
exit


