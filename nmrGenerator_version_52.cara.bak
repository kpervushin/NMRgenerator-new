#include &lt;Avance.incl&gt;
#include &lt;Grad.incl&gt;
#include &lt;Delay.incl&gt;


"p2=p1*2"
"p22=p21*2"
"d0=3u"
"d11=30m"
"d13=4u"

"d21=5.5m"
;"d23=12m"
"d26=2.5m"


"in29=in10"
"in30=in10"

"d10=d23/2-p14/2"
"d29=d23/2-p14/2-p26-d21-4u"
"d30=d23/2-p14/2"


"DELTA=d0*2+larger(p14,p22)-p14"
"DELTA1=d23-d21-p26"
"DELTA2=d26-p16-d16-p11-12u"


"spoff2=0"
"spoff3=0"
"spoff5=(bf2*(cnst21/1000000)-o2)"
"spoff8=0"


aqseq 321


1 d11 ze
  d11 pl16:f3 
2 d11 do:f3
3 d1 pl1:f1
  10u pl4:f4
  p1 ph20
  d26 pl3:f3
  (center (p2 ph20) (p22 ph20):f3 )
  d26 UNBLKGRAD
  (p1*200 ph20):f1
  (p1 ph21):f1 

  
  10u
  p16:gp1
  d16

  (p21 ph8):f3
; N -transverse

  d30
  (center (p14:sp3 ph20):f2 (p22 ph20):f3 )
  d10

  (p3 ph4):f2 
  d0
  (center (p14:sp5 ph20):f2 (p22 ph20):f3 )
  d0
  (p3 ph5):f2

  d30
  (center (p14:sp3 ph20):f2 (p22 ph20):f3 )
  d10


  (p21 ph20):f3

  p16:gp3
  d16 
  (p11:sp1 ph22):f1
  4u
  4u pl1:f1

  (p1 ph20):f1 
  4u
  p16:gp4
  d16
  DELTA2 pl0:f1
  (p11:sp1 ph22):f1
  4u
  4u pl1:f1
  (center (p2 ph20) (p22 ph20):f3 )
  4u pl0:f1
  (p11:sp1 ph22):f1
  4u
  DELTA2
  p16:gp4
  d16 pl16:f3
  4u BLKGRAD
  go=2 ph31 cpd3:f3 
  d11 do:f3 mc #0 to 2 
     F1PH(rd10 &amp; rd29 &amp; rd30 &amp; rp5 &amp; ip4, id0) 
     F2PH(ip5, id10 &amp; id29 &amp; dd30)
exit


