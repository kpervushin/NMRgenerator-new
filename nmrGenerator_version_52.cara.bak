"l0=1"


"spoff2=0"
"spoff3=0"
"spoff5=bf2*(cnst21/1000000)-o2"
"spoff8=0"


aqseq 321


1 d11 ze
  d11 LOCKDEC_ON
  d11 H2_PULSE
  d11 pl17:f4

2 d11
3 d11 H2_LOCK
  9m LOCKH_OFF
  d1 pl1:f1
  50u UNBLKGRAD
