<code lang='Lua'>local d = 1 -- A, immersion depth
local Rb = 7 -- A , radius of pentamer
local Rm =16 -- A, radius of micelle

local a = 0.4 -- A, fraction of micelle accessible for the probe
local d_xyz = 0.1 --A, integration step
local i_max =200 -- size of the greed
function r_prime(x,y,z)
local z_r_prime = z
local y_r_prime = y
local x_r_prime = x-(Rb-d)
return x_r_prime,y_r_prime,z_r_prime
end
function vlength(x,y,z)
return math.sqrt(x^2+y^2+z^2)
end

function pre(x,y,z)
return (math.exp(-vlength(x,y,z)/(a*Rm)))/((vlength(r_prime(x,y,z)))^6)
--return (math.exp(-vlength(x,y,z)/(a*Rm)))
end

function loclize(x,y,z)  --if outside cylinder
if(x^2+y^2 &lt; Rb^2) then
return 0
else
return 1
end
end

