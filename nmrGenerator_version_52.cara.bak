<fld name='Body' type='Memo'>;plop15N13C.eth
;dreier, February 2002
; modified gsw 0203

;Program for determining 15N and 13C 180 degree hard pulse length 
;on labeled proteins
;- read parameter plop15N13C.eth
;- set 90 degree pulse on 1H
;  p6: 150 degree 15N
;  p4: 150 degree 13C
;- zg
;- determine phase of spectra
;- use "xplop" for plot the result
;  (the plot can be changed by typing "Enter" in the black window)

;pl1: power level for hard 1H pulses p1/p2
;pl2: power level for 13C hard pulses p4
;pl3: power level for hard 15N pulses p6
;pl9: power level presaturation

;p1: 1H 90 deg pulse at pl1
;p4: 13C 170 deg pulse at pl2
;p6: 15N 170 deg pulse at pl3

;gpz0: 50%
;gpz1: 10%
;gpz2: 15%
;gpz3: 20%
;gpz4: 30%
;gpz5: 60%
;gpz6: 35%
;gpz7: 40%

#define GRADIENT0 10u p20:gp0 200u
#define GRADIENT1 10u p21:gp1
#define GRADIENT2 10u p21:gp2
#define GRADIENT3 10u p21:gp3
#define GRADIENT4 10u p21:gp4
#define GRADIENT5 10u p21:gp5
#define GRADIENT6 10u p21:gp6
#define GRADIENT7 10u p21:gp7

"d20= 400u" ; maximal sum of p4 and p6

#include &lt;Avance.incl&gt;
"p2=2*p1"
"p20=1m"
"p21=0.39m"
"d1=1s"
"d2=2.3m"
"d10=d2/4-(p21+10u+p2/2)"
"d11=1u"
"d12=1u"
"d21=p21"


1 10u ze
  10u   
2 100u
3 100u 
4 100u
5 LOCKH_OFF
  "d11=d2/4-(p21+10u+p6/2)"
  5u
  "d12=d2/4+d20/2-(p21+10u+p4/2)"
  10u pl9:f1
  d1 cw:f1
  10u do:f1
  20u LOCKH_ON
  GRADIENT0
  20u pl1:f1 
  20u pl2:f2 
  20u pl3:f3
  1m
  (p1 ph2):f1
  GRADIENT1
  d21
  d10
  (d10 p2 ph21 d10):f1 (d11 p6 ph1):f3 (d12 p4 ph1):f2
  GRADIENT1
  GRADIENT2
  (d10 p2 ph21 d10):f1 (d11 p6 ph1):f3 (d12 p4 ph1):f2
  GRADIENT2
  GRADIENT3
  (d10 p2 ph21 d10):f1 (d11 p6 ph1):f3 (d12 p4 ph1):f2
  GRADIENT3
  d10
  d21
  (p1 ph21)
  GRADIENT4
  (p6*0.5 ph20 2u p6*0.5 ph3):f3
  10u
  (p4*0.5 ph20 2u p4*0.5 ph3):f2
  (p1 ph21):f1 
  GRADIENT5
  d10
  d21
  (d10 p2 ph21 d10):f1 (d11 p6 ph1):f3 (d12 p4 ph1):f2
  GRADIENT5
  GRADIENT6
  (d10 p2 ph21 d10):f1 (d11 p6 ph1):f3 (d12 p4 ph1):f2
  GRADIENT6
  GRADIENT7
  (d10 p2 ph21 d10):f1 (d11 p6 ph1):f3 (d12 p4 ph1):f2
  GRADIENT7
  d10
  d21
  go=2 ph0
  100u wr #0 if #0 ipu4 zd
  20u ipu6
  if "(p4+p6)&gt;d20" goto 9
  lo to 5 times td1
9 10u do:f1
  10u do:f2
  10u LOCKH_OFF 
exit

ph1=0
ph2=0 0 2 2
ph3=0 2
ph20=0
ph21=1
ph22=2
ph23=3
ph25=0
ph26=0
ph0=0 2 2 0

