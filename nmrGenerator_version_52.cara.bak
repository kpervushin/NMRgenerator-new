prosol relations=&lt;triple&gt;


#include &lt;Avance.incl&gt;
#include &lt;Grad.incl&gt;
#include &lt;Delay.incl&gt;


"p2=p1*2"
"p22=p21*2"
"d0=in0/2-p21*2/3.14159-p1+20u"

"d26=1s/(cnst4*4)"

"d17=2.77m-p11-p19-d16"

"DELTA1=p16+d16+8u"
"DELTA2=d24-p19-d16"
"DELTA3=d26-p16-d16"


"DELTA=p16+d16"

