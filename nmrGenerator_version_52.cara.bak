<obj oid='164'>
<fld name='Type' type='String'>Pulse Program</fld>
<fld name='Author(s)' type='String'>K.Pervushin</fld>
<fld name='Datum' type='String'>2-2-2008</fld>
<fld name='Body' type='Memo'>;Read 4096 re-matrix entries
;Read 4096 im-matrix entries
; -1.800   2.354  -0.539   1.571   1.911   3.289   1.618   0.543   1.494   2.104   0.900   1.665  -0.544   1.647   5.042  -0.899  -1.456  -1.027   1.571   3.136   4.040   1.570   0.750   0.750  
; --- TRANSFORM-------------------------------------
; F2 PHC 90 0, F1 PHC 0 -180, cocain AP/IN change ph28 by 1, change F1 PHC by 90 grad
; --- END      -------------------------------------
#include &lt;Avance.incl&gt; 
#define GRADIENT1  10u p21:gp1 200u
#define GRADIENT2  10u p22:gp2 200u
#define GR_DP 10u p29:gp9*plusminus 200u
#define GR_RP 10u p30:gp10 200u
define delay DP_COMP
"DP_COMP=p29+210u"
define delay RP_COMP
"RP_COMP=p30+210u"
"d0=in0/2-p3*2/3.14159-p1"  
"d2=1.78m"; set d2 =1/4J if J=140 Hz 
"l2 = td1/2"  
 1  10u ze 
 2  10u do:f2
 
 10u pl9:f1
3   d1 cw:f1
10u do:f1
 10u pl1:f1 
   10u pl2:f2 
   10u LOCKH_ON  
GRADIENT1
(p1 ph30):f1
     d2
(p1*2 ph30):f1 (p3*2 ph30):f2 
     d2
(p1 ph29):f1 
GRADIENT2
(p3 ph28):f2 
                  (d0 p1*2 ph30 d0):f1 
DP_COMP
(p3*2 ph30):f2
GR_DP
(p1*1.499 ph0):f1 (p3*1.000 ph2):f2  ;  x1:  2.354 x0: -1.800 x3:  1.571 x2: -0.539 
           d2*0.608; x5/2:  0.956
           (p1*2 ph30):f1 (p3*2 ph30):f2 
           d2*0.608
(p1*1.030 ph5):f1 (p3*0.951 ph7):f2  ;  x1:  1.618 x0:  3.289 x3:  1.494 x2:  0.543 
           d2*0.670; x5/2:  1.052
           (p1*2 ph30):f1 (p3*2 ph30):f2 
           d2*0.670
(p1*1.060 ph10):f1 (p3*1.049 ph12):f2  ;  x1:  1.665 x0:  0.900 x3:  1.647 x2: -0.544 
           d2*1.605; x5/2:  2.521
           (p1*2 ph30):f1 (p3*2 ph30):f2 
           d2*1.605
(p1*1.073 ph15):f1 (p3*1.000 ph17):f2  ;  x1: -1.456 x0: -0.899 x3:  1.571 x2: -1.027 
           d2*0.998; x5/2:  1.568
           (p1*2 ph30):f1 (p3*2 ph30):f2 
           d2*0.998
(p1*0.999 ph20):f1 (p3*0.477 ph22):f2 
20u
RP_COMP 
(p1*2 ph30):f1
GR_RP
20u pl12:f2 LOCKH_OFF
go=2 ph31 cpd3:f2 
1m do:f2 mc #0 to 2 F1EA(ip2*180 &amp; igrad plusminus, id0)
10u do:f1
10u do:f2
exit
ph29=1
ph30=0
ph28=0 2; S-excitation pulse
ph31=0 2 ; receiver
ph0=(360) 76 ; x0: -1.800000
ph2=(360) 149 ; x2: -0.539000
ph5=(360) 8 ; x0: 3.289000
ph7=(360) 31 ; x2: 0.543000
ph10=(360) 51 ; x0: 0.900000
ph12=(360) 148 ; x2: -0.544000
ph15=(360) 128 ; x0: -0.899000
ph17=(360) 121 ; x2: -1.027000
ph20=(360) 51 ; x0: 4.040000
ph22=(360) 42 ; x2: 0.750000
;EOF input
</fld>
