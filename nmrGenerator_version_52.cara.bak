<obj oid='62'>
<fld name='Macro Name' type='String'>tocsy_clean</fld>
<fld name='Type' type='String'>Macro</fld>
<fld name='Author(s)' type='String'>K.Pervushin</fld>
<fld name='Datum' type='String'>20-12-2007</fld>
<fld name='Body' type='Memo'>exp_environment = {
d9 =  0.07, --isotropic_mixing_time
d1=1.,

["pulprog"]= "clean-tocsy_wg.pk",
o1 = spectrometer[now].proton_carrier_298k, 
o2p=spectrum.CaCb.carrier,
o3p=spectrum.N.carrier,
o4p=spectrum.D.carrier, 
["xcpr"]="parmode 2D",

-- set hard pulses
p1 = 16*700.13/spectrometer[now].B_field, --16 us for 90 grad to prevent heating during mix
pl1= calc_power_set(16*700.13/spectrometer[now].B_field,1.0,spectrometer[now].calibration_pulse90_f1,spectrometer[now].calibration_power_db_f1),
pl19 =60, --water presat (when used) 
pcpd2 = pulses.Ccpd90,
pl12 = calc_power_set(pulses.Ccpd90,1.0,spectrometer[now].calibration_pulse90_f2,spectrometer[now].calibration_power_db_f2),
["cpdprg2"]="garp4",
pcpd3 = pulses.Ncpd90,
pl13 = calc_power_set(pulses.Ncpd90,1.0,spectrometer[now].calibration_pulse90_f3,spectrometer[now].calibration_power_db_f3),
["cpdprg3"]="waltz16",

 --set soft powers and pulses
p11 = pulses.watergate90, -- assume that watergate90 mks is ideal for 700
["spnam2"]= "Gauss_5_256.pk", -- integral factor 0.5
 sp2= calc_power_set(pulses.watergate90,0.5,spectrometer[now].calibration_pulse90_f1,spectrometer[now].calibration_power_db_f1),
 spoal2 =0.5, -- have to check what's that
 spoffs2=0,

--HNselective watergate
p12 =pulses.watergate90, -- it is critical, assume 300 is ideal for 700
["spnam1"]= "Gauss_5_256.pk", -- integral factor 0.5
 sp1 = calc_power_set(pulses.watergate90,0.5,spectrometer[now].calibration_pulse90_f1,spectrometer[now].calibration_power_db_f1),
 spoal1=0.5, -- have to check what's that
 spoffs1 =0, -- it is critical, assume 300 is ideal for 700
 

 
  in0 = 0.5/(12.*spectrometer[now].B_field), -- sw=12 ppm is assumed suitable for 700
 ["1 in_010 "]= 0.5/(12.*spectrometer[now].B_field),
 --set number of sampled points
 ["1 td "]=(320), --H direction
 ["2 td "]=(1024), --H direction
 ["1 nd_010"]= 2 , --H direction 
 --set gradients
gpnam1 ="SINE.100",
gpnam2 ="SINE.100",
gpz1 =80,
gpz2 =90,

p21 =1000, --(Crusher)
p22 =1000, --(watergate)
--acqu modes
["1 FnMODE"]= "States-TPPI",

--processing
["1 mc2"]= "States-TPPI",


["2 stsi"]= 0,
["2 si"]= 4096, 
["1 si"]= 1024, -- H dim
["1 phc0"]= 45, -- N dim
["1 phc1"]= -180, -- N dim 
["1 sf "]=spectrometer[now].B_field, -- H dim  
["1 sr "]=0, -- N dim
["2 sr "]=0, -- N dim 
["1 ssb"]=  2, -- n dim
["2 ssb"]=  2, -- n dim
["1 WDW"]=  "QSINE", -- n dim
["2 WDW"]=  "QSINE", -- n dim

["1 PH_mod"]=  "pk", -- n dim
["2 PH_mod"]=  "pk", -- n dim 
["1 REVERSE"]="FALSE",
["2 REVERSE"]="FALSE",
["2 BC_mod"]="qfil",
["2 BC_mod"]="no"
}


