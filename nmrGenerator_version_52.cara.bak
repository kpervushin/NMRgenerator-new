--print(vlength(20,30,40))
--print(x1.."  "..y1.."  "..z1.." len "..vlength(x1,y1,z1))

--print("loclize "..loclize(6.9,0,0))

function pre_sum(dd)
local accu=0
for i = 1, i_max do
for j = 1, i_max do
for k = 1, i_max do

accu=accu+loclize(i*d_xyz,j*d_xyz,k*d_xyz)*(d_xyz^3)*pre(i*d_xyz,j*d_xyz,k*d_xyz)

--accu=accu+d_xyz^3
end
end
end

return accu*4
end --function pre_sum(dd)

