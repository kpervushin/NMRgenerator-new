<obj oid='138'>
<fld name='Type' type='String'>Pulse Program</fld>
<fld name='Author(s)' type='String'>K.Pervushin</fld>
<fld name='Datum' type='String'>17-1-2008</fld>
<fld name='Body' type='Memo'>;TR_HNCA.new
;rh, sb, Jan. 2003

;M. Salzmann et. al., J. Am. Chem. Soc. 121 (4) 844-848 (1999)

;carrier positions [ppm]: 1H 4.7, 13C 55, 15N 119, 2H 3.8

;pl1   : power 1H 90 hard p1
;pl3   : power 15N 90 hard p5
;pl2   : power 13CaCb 90 hard p3
;pl4   : power 2H 90 hard pcpd4
;sp1   : power 1H 90 gaussian p11
;sp15  : power 13C 90 seduce p31

;p1    : 90 1H hard [~10us]
;p2    : 180 1H hard [~20u]
;p3    : 90 13CaCb hard [~12us]
;p4    : 180 13CaCb hard [~24u]
;p5    : 90 15N hard [~40us]
;p6    : 180 15N hard [~80us]
;p11   : 90 1H gauss128_5 [1ms]
;p31   : 90 13CO seduce1.ms [300us]
;pcpd4 : 90 2H waltz [~330us]
;cpds2 : 13CO decoupling (cpdprg2: sed_p31.eth, spnam15: seduce1.eth)

;in0   : 15N increment t1/2 [208us]
;in10  : 13C increment t1/2 [52us]
;l1    : loop counter for TROSY (do not touch)

;d1    : interscan delay
;d2    : 1/2 1H-15N inept delay [2.7ms]
;d3    : 1/2 15N-13Ca inept delay [9-12ms]
;d30   : calculated during runtime
;d31   : calculated during runtime

;cnst21: CO chemical shift [175ppm]
;cnst22: CA chemical shift [55ppm]

;gpz1  : 26%
;gpz2  : 70%
;gpz3  : 43%
;gpz4  : 65%

;p21   : 800u (Gradient in first INEPT)  
;p22   : 1m   (Z-Filter betw. HN/N transfer)
;p23   : 800u (Gradient in N/HN INEPT)
;p24   : 800u (Gradient in WG)

"l1=1"

"p2 = p1*2"
"p4 = p3*2"
"p6 = p5*2"

"d0=10u"
"d6=(d3*2+p6)-p11-30u"
"d10=in10*0.5-p4/3.14159-16u-p2"
"d14=d3-d0"
"d15=d3-d0-10u"
"d29=p5-p3"

define delay INEPT_1
define delay TRHN
define delay TRWG
define delay INEPTnc

"INEPT_1=d2-p11-p21-210u"
"TRHN=d2-p23-200u"
"TRWG=d2-p11-p24-210u"
"INEPTnc=d3-10u"

"spoff15=bf2*((cnst21)/1000000)-o2"

#define GRADIENT1  10u p21:gp1 190u
#define GRADIENT2  10u p22:gp2 190u
#define GRADIENT3  10u p23:gp3 190u
#define GRADIENT4  10u p24:gp4 190u
#define GRADIENT5  10u p25:gp5 190u

#include &lt;Avance.incl&gt;

aqseq 312

1 10u ze
;----------------------------------------switch on 2D Decoupling Hardware
  10m LOCKDEC_ON
2 1m
  "d30=d3-10u-(d0)"
  100u
  "d31=d3-(d0)"
  100u
  20u H2_LOCK
  10u LOCKH_OFF
  d1
  1m do:f2 do:f3 do:f4
  20u pl1:f1
  20u pl3:f3
  20u pl2:f2
  20u pl4:f4
  20u LOCKH_ON
  20u H2_PULSE
;-----------------------------------------First Inept
  (p1 ph20):f1
  10u
  (p11:sp1 ph22:r):f1
  GRADIENT1
  INEPT_1 pl1:f1
  (center (p2 ph21):f1 (p6 ph20):f3)
  GRADIENT1
  INEPT_1
  (p11:sp1 ph23:r):f1
  4u
  6u pl1:f1
  (p1 ph23):f1
  GRADIENT2
  (10u cpds2):f2
;-------------------------------------------------- INEPT N/CA
  (p5 ph20):f3
  INEPTnc
  (center (p6 ph20):f3 (10u do 10u pl2 p4 ph20 10u cpds2 10u):f2)
  INEPTnc
  (p5 ph21):f3
  10u cpd4:f4
  10u do:f2 pl2:f2
  (p3 ph2):f2
;-------------------------------------------------- 13CA evolution
  4u cpds2:f2
  d10*0.5
  (p2 ph20):f1
  d10
  (p2 ph20):f1
  d10*0.5
  (6u do 6u pl2):f2
  (p3 ph9):f2
;-------------------------------------------------- INEPT CA/N
  10u do:f1 do:f4
  10u cpds2:f2
if "l1==1" goto 41
  (p5 ph17):f3
goto 42
41 (p5 ph18):f3
;-------------------------------------------------- 15N evolution

42 (d6 pl1 10u p11:sp1 ph27:r):f1 (d31 p6 ph20 d14 d0):f3 (d30 10u do d29 pl2 p4 ph20 d29 10u cpds2 d15 d0):f2

;-------------------------------------------------- INEPT N/HN
   10u pl1:f1
  (p1 ph10):f1
  10u do:f2
  GRADIENT3
  TRHN pl2:f2
  (center(p2 ph20):f1 (p6 ph20):f3 (p4 ph20):f2)
  10u
  GRADIENT3
  TRHN
  (center(p1 ph20):f1 (p5 ph12):f3)
  GRADIENT4
  TRWG
  (p11:sp1 ph22:r):f1
  4u
  6u pl1:f1
  (center(p2 ph20):f1 (p6 ph20):f3)
  10u
  (p11:sp1 ph22:r):f1
  GRADIENT4
  TRWG LOCKH_OFF
  (p5 ph11):f3
;-------------------------------------------------- Acquisition
   go=2 ph31
   1m mc #0 to 2
      F1EA(ip10*2 &amp; ip12*2 &amp; iu1, ru1 &amp; id0)
      F2PH(rp10 &amp; rp12 &amp; ru1 &amp; rd0 &amp; dp9, id10)


10u do:f1
10u do:f3
10u do:f2
10u do:f4
10m LOCKH_OFF
10m LOCKDEC_OFF

exit

ph2 =0 0 0 0  2 2 2 2
ph9 =0
ph10=3 3 3 3
ph11=0 0 0 0
ph12=3 3 3 3

ph17=1 3 2 0
ph18=1 3 0 2

ph20=0
ph21=1
ph22=2
ph23=3
ph27=0

ph31=1 3 2 0  3 1 0 2

;##/($P[1],$PL[1])= &amp;SetPulse(f1,HP,90);
;##/($P[3],$PL[2])= &amp;SetPulse(f2,HP,90);
;##/($P[5],$PL[3])= &amp;SetPulse(f3,HP,90);
;##/$P[11]= &amp;SetShape(f1,1,0.001,90);
;##/$P[31]= &amp;SetShape(f2,15,0.0003,90);
</fld>
<fld name='Dimension' type='String'>3D</fld>
<fld name='Group' type='String'>assignment backbone</fld>
<fld name='Subgroup' type='String'>ETH</fld>
<fld name='Memo' type='String'>---</fld>
