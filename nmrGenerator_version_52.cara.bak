<fld name='Body' type='Memo'>nucleus[1]=H
nucleus[2]=N
nucleus[3]=C

d[t_] := delay[t,{{1,2},{2,3}}]
r1=1/(2*j[1,2])
r2=1/(2*j[2,3])

phi1 = { y, y, y, y}
phi2 = {-y,-y,-y,-y}
phi3 = { x, x, x, x}
phi7 = { x, x,-x,-x}
phi8 = {-x,-x,-x,-x}
phi9 = { x, x, x, x}
psi1 = { y,-x, y,-x}
psi2 = {-y,-y,-y,-y}
psi3 = {-x,-x,-x,-x}
rec  = { y, x,-y,-x}


exp := spin[1,z] + spin[2,z] + spin[3,z] //
po2sto[{{1,2}}] //

pulse[90,phi8,{1}] //
	d[r1/2] //
pulse[180,phi9,{1,2}] //
	d[r1/2] //

pulse[ 90,phi1,{1}]  // 	(*phi1 y*) 
pulse[ 90,psi1,{2}]  // 	(*psi1 y -y -x x*)
        d[r2/2+t2/4-t1/4] //
pulse[180,phi9,{2,3}] //
        d[r2/2-t2/4+t1/4] //
pulse[90,phi9,{3}] //		(*psi1 y -y x -x*)
	d[t2] // 
pulse[90,phi7,{3}] //
        d[r2/2-t2/4+t1/4] //
pulse[180,phi9,{2,3}] //
        d[r2/2+t2/4-t1/4] //
pulse[ 90,phi2,{1}]  //  	(*phi2 y*)
	d[r1/2] //
pulse[ 180,phi3,{1,2}]  //
	d[r1/2] //
pulse[ 90,phi3,{1}]  // 	(*phi3 x*) 
pulse[ 90,psi2,{2}]  //  	(*psi2 y*)
	d[r1/2] //
pulse[ 180,phi3,{1,2}]  //
	d[r1/2] //
pulse[ 90,psi3,{2}] // 	(*psi3 x*)


	receiver[rec] //    	(*rec y -y -x x*)
        observable //
	Simplify
	
	exp



