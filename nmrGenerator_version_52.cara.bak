 p16 = 1500, --15N encoding gradient gp2
 p19 = 500,
 d0 = 3e-6,

 d16 = 200e-6,
 d24= 1/(4.*couplings.J_HN.Hz),
 d26 = 1/(4.*couplings.J_HN.Hz),
 cnst4 = couplings.J_HN.Hz,
 in0 = (0.5/(spectrum.N.sw*spectrometer[now].B_field*IndirectChemicalShiftReferences.N15)),
 ["2 in_010 "]= (0.5/(spectrum.N.sw*spectrometer[now].B_field*IndirectChemicalShiftReferences.N15)),
 ["1 td "]=16,
 ["2 td "]=(128), --H direction
 ["3 td "]=(1024), --H direction
 ["1 nd_010"]= 2 , --n direction 
 ["2 nd_010"]= 2 , --n direction 
 --acqu modes
["1 FnMODE"]= "QF",
["2 FnMODE"]= "Echo-Antiecho",
 
["gpnam1"]="SINE.100",
["gpnam2"]=" SINE.100",
["gpnam3"]=" SINE.100",
["gpnam4"]=" SINE.100",
["gpnam5"]=" SINE.100",
["gpnam6"]=" SINE.100",
["gpnam7"]=" SINE.100",
["gpnam21"]=" SINE.100",
 
gpz1 =50,
gpz2 =40,
gpz21 =-40,
gpz3 =8.1, --20.1
gpz4 =5,
gpz5  =-2,
gpz6  =60,
gpz7  =70,

--processing
["1 mc2"]= "QF",
["2 mc2"]= "echo-antiecho",
["3 absf1"]=  12, 
["3 absf2"]=  6, 
["3 si"]=  2048,  --H dim
["3 stsi"]=  1024,
["2 si"]=  512, -- n dim 
["1 si"]=  16, -- n dim 
["2 phc0"]=  0, --0, -- n dim
["2 phc1"]=  -180, ---180, -- n dim 
["2 WDW"]=  "QSINE", -- n dim
["3 WDW"]=  "QSINE", -- n dim
["3 ssb"]=  2, -- n dim
["2 ssb"]=  2, -- n dim 
["2 PH_mod"]=  "pk", -- n dim
["3 PH_mod"]=  "pk", -- n dim 
["2 REVERSE"]="FALSE",
["2 sf"]=spectrometer[now].B_field/(9.8), -- C dim 
["3 sr"]=0,  --referencing relative to absolute spectormeter calibration
["2 sf"]=spectrometer[now].B_field*IndirectChemicalShiftReferences.N15 -- N dim referencing relative to DSS
}

VCPMGList = { 4., 24., 2., 4., 20., 6., 52., 8., 16., 10., 40., 6., 18., 14., 2., 30.}
for key, value in pairs(VCPMGList) do
print("Plane: "..key.."   CPMG: "..(1/(4*(exp_environment.d12*0.25/value-exp_environment.p21*1e-6))).."\n")
end


