<obj oid='150'>
<fld name='Type' type='String'>Pulse Program</fld>
<fld name='Author(s)' type='String'>K.Pervushin</fld>
<fld name='Datum' type='String'>26-1-2008</fld>
<fld name='Body' type='Memo'>;hmqcf3gpph19
;avance-version (02/05/31)
;HMQC
;2D H-1/X correlation via heteronuclear zero and double quantum
;   coherence
;phase sensitive
;with decoupling during acquisition
;using f3 - channel
;water suppression using 3-9-19 pulse sequence with gradients
;
;M. Piotto, V. Saudek &amp; V. Sklenar, J. Biomol. NMR 2, 661 - 666 (1992)
;V. Sklenar, M. Piotto, R. Leppik &amp; V. Saudek, J. Magn. Reson.,
;   Series A 102, 241 -245 (1993)
;
;$CLASS=HighRes
;$DIM=2D
;$TYPE=
;$SUBTYPE=
;$COMMENT=


#include &lt;Avance.incl&gt;
#include &lt;Grad.incl&gt;
#include &lt;Delay.incl&gt;


"p2=p1*2"
"p22=p21*2"
"d0=in0/2-p1*2/3.14159"
"d26=1s/(cnst4*2)"
"d21=d26-p21-p11-p26-200u"

#define GRADIENT1   10u p26:gp6 200u
#define GRADIENT0   10u p26:gp2 200u

aqseq 312


1 ze 
  30m pl16:f3
2 10u pl19:f1
  10u do:f3
  d1 cw:f1
  20u do:f1
  20u pl1:f1 pl2:f2 pl3:f3
  20u LOCKH_ON
;  10u pl16:f3
;  10u cpd3:f3
;--NOESY
;--------------1H evolution
(p1 ph10 d0 d0 p1 ph11):f1 (d0 p3*2 ph20):f2 (d0 p21*2 ph20):f3
;  10u do:f3
;  10u pl3:f3
;--------------mixing time
  GRADIENT0
  d8 
;  GRADIENT0


;  (p11:sp1 ph27):f1 
  10u
 (p1 ph1):f1
  20u
  d21 
  GRADIENT1 
  (center (p11:sp1 ph26:r 10u p2 ph20 10u p11:sp1 ph26:r):f1 (p21 ph3 4u p21 ph4):f3 )
  GRADIENT1
  d21 pl16:f3
  20u LOCKH_OFF
  go=2 ph31 cpd3:f3
  d1 do:f3 mc #0 to 2 
  F1PH(ip3*2,id10)
  F2PH(ip11, id0)
exit 
  

ph27=2

ph3=0 ; H-15N selection
ph4=0
ph31=0 2 2 0 1 3 3 1 
ph1= 0 0 0 0 3 3 3 3  ;NOESY
ph10=0 2 0 2
ph11=0 0 2 2 

;ph1 =0
;ph3=0 2
;ph4=0 0 2 2
;ph31=0 0 0 0 2 2 2 2  2 2 2 2 0 0 0 0
;ph10=0 0 0 0 2 2 2 2
;ph11=0 0 0 0 0 0 0 0  2 2 2 2 2 2 2 2

ph26=2
ph20=0


;pl1 : f1 channel - power level for pulse (default)
