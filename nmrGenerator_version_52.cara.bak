<fld name='Author(s)' type='String'>K.Pervushin</fld>
<fld name='Datum' type='String'>20-12-2007</fld>
<fld name='Body' type='Memo'>exp_environment = {
d2= 1/(4.*couplings.J_HaCa.Hz),
d1 =1.,
d8 =0.2, -- (s) mixing time

["pulprog"]= "NSY_hsqc13C_pfg.pk",

-- set carrier 
o1 = spectrometer[now].proton_carrier_298k, 
o2p=spectrum.CaCb.carrier, 
o3p=spectrum.N.carrier,
o4p=spectrum.D.carrier, 
["xcpr"]="parmode 3D",


--set hard powers and pulses for f1-f4
p1 = spectrometer[now].calibration_pulse90_f1,
pl1= spectrometer[now].calibration_power_db_f1,
p3  = spectrometer[now].calibration_pulse90_f2,
pl2= spectrometer[now].calibration_power_db_f2,
p5 = spectrometer[now].calibration_pulse90_f3,
pl3 =spectrometer[now].calibration_power_db_f3,
pcpd4 = pulses.Dcpd90, -- D decoupling
pl4 = calc_power_set(pulses.Dcpd90,1.0,spectrometer[now].calibration_pulse90_f4,spectrometer[now].calibration_power_db_f4),
["cpdprg4"]= "waltz16", 
pl19 =60, --water presat (when used) 
pcpd2 = pulses.Ccpd90,
pl12 = calc_power_set(pulses.Ccpd90,1.0,spectrometer[now].calibration_pulse90_f2,spectrometer[now].calibration_power_db_f2),
["cpdprg2"]="garp4",
pcpd3 = pulses.Ncpd90,
pl13 = calc_power_set(pulses.Ncpd90,1.0,spectrometer[now].calibration_pulse90_f3,spectrometer[now].calibration_power_db_f3),
["cpdprg3"]="waltz16",

--CO decoupling
 p12 =pulses.COdec90, -- it is not that critical, assume 300 is ideal for 700
 ["spnam12"] ="Gauss_5_256.pk", -- integral factor 0.5
 sp12 = calc_power_set(pulses.COdec90,0.5,spectrometer[now].calibration_pulse90_f2,spectrometer[now].calibration_power_db_f2),
 spoal12 =0.5, -- have to check what's that
 cnst21 = spectrum.CO.carrier,

--set increment times
 in0 = (0.5/(12*spectrometer[now].B_field/1.)), -- H dim
 ["1 in_010 "]= (0.5/(12*spectrometer[now].B_field/1.)), -- H dim
 
 in10 = (0.5/(spectrum.CaCb.sw*spectrometer[now].B_field*IndirectChemicalShiftReferences.C13)), -- C dim
 ["2 in_010 "]= (0.5/(spectrum.CaCb.sw*spectrometer[now].B_field*IndirectChemicalShiftReferences.C13)),
  --set number of sampled points
 ["1 td "]=320,
 ["2 td "]=(0.5/couplings.J_CC.Hz)/((0.5/(spectrum.CaCb.sw*spectrometer[now].B_field*IndirectChemicalShiftReferences.C13))), --C direction, limited by CC coupling, 8 ms is max Ca, signal goes to 0 in 1/(2*Jcacb)=13ms
 ["3 td "]=(1024), --H direction
 ["1 nd_010"]= 2 , --H direction 
 ["2 nd_010"]= 2 , --C direction 
 


 --gradients
["gpnam0"]="SINE.100",
["gpnam1"]="SINE.100",
["gpnam2"]=" SINE.100",
["gpnam3"]=" SINE.100",
["gpnam4"]=" SINE.100",
["gpnam9"]=" SINE.100",
["gpnam10"]=" SINE.100",
 
gpz0 =80,
gpz1 =19,
gpz2 =90,
gpz3 =85,
gpz4 =30,
gpz9  =90,
gpz10 =90, --plus or minus critical

p20= 1000, --(crusher)
p21= 800, --(Gradient in first INEPT)
p22= 1000, --(Gradient for 1st z-filter)
p23= 1000, --(Gradient for 2nd z-filter)
p24= 800, --(Gradient for 3rd z-filter)
p29= 995, --PFG 13C
p30= 250, --PFG 1H

--acqu modes
["1 FnMODE"]= "States-TPPI",
["2 FnMODE"]= "Echo-Antiecho",

--processing
["1 mc2"]= "States-TPPI",
["2 mc2"]= "echo-antiecho",
["2 absf1"]=  4.4, 
["2 absf2"]=  -1, 
["1 si"]=  256,  --H dim
["1 stsi"]=  0,
["2 si"]=  128,  --H dim
["2 stsi"]=  0,
["3 si"]=  2048,  --H dim
["3 stsi"]=  0,
["1 phc0"]=  90, -- h dim
["1 phc1"]=  -180, -- h dim 
["2 phc0"]=  -36.05,-- 0, -- c dim
["2 phc1"]=  -112.8,-- -180, -- c dim 
["3 ssb"]=  4, -- n dim
["2 ssb"]=  2, -- n dim
["1 ssb"]=  4, -- n dim 
["1 PH_mod"]=  "pk", -- n dim
["2 PH_mod"]=  "pk", -- n dim 
["3 PH_mod"]=  "pk", -- n dim 
["1 WDW"]="QSINE",
["2 WDW"]="QSINE",
["3 WDW"]="QSINE",
["1 sf"]=spectrometer[now].B_field/(1), -- h dim 
["2 sf"]=spectrometer[now].B_field/(3.9769), -- c dim 


["aqorder"]= "3-1-2",
["phcor11"]=45, 

["1 absf1"]= 10, 
["1 absf2"]= -1, 
["2 absf1"]= 100, 
["2 absf2"]= -100, 
["3 absf1"]= 4.4, 
["3 absf2"]= -1 ,
["2 REVERSE"]="TRUE"
}
</fld>
