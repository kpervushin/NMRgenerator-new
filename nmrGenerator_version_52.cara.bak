<obj oid='134'>
<fld name='Type' type='String'>Pulse Program</fld>
<fld name='Author(s)' type='String'>Bruker</fld>
<fld name='Datum' type='String'>17-1-2008</fld>
<fld name='Body' type='Memo'>;roesyphpr
;avance-version (02/11/27)
;2D ROESY with cw spinlock for mixing
;phase sensitive
;
;A. Bax &amp; D.G. Davis, J. Magn. Reson 63, 207-213 (1985)
;
;$CLASS=HighRes
;$DIM=2D
;$TYPE=
;$SUBTYPE=
;$COMMENT=


#include &lt;Avance.incl&gt;


"d11=30m"
"d12=20u"
"d13=4u"


"d0=in0/2-p1*2/3.1416-4u"

#define GRADIENT2   10u p22:gp2 200u


1 ze
2 d11
3 d12 pl9:f1
  d1 cw:f1 ph29
  d13 do:f1
  d12 pl1:f1
  p1 ph1
  d0 
  4u pl11:f1
  p15 ph2
  
  GRADIENT2 
  (p12:sp1 ph6):f1
  10u
  10u pl1:f1
  20u
  (p1*2 ph20:r):f1
  30u
  (p12:sp1 ph6):f1
  GRADIENT2 

  go=2 ph31
  d11 mc #0 to 2 F1PH(ip1 &amp; ip29, id0)
exit


ph1=0 2 2 0 1 3 3 1 
ph2=0 2 0 2 1 3 1 3
ph29=0
ph31=0 2 2 0 3 1 1 3
ph6 =2
ph20=0


