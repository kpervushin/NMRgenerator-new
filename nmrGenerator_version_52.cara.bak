<fld name='Author(s)' type='String'>K.Pervushin</fld>
<fld name='Datum' type='String'>2-2-2008</fld>
<fld name='Body' type='Memo'>
exp_environment = {
d2= 1/(4.*couplings.J_HaCa.Hz),
d1 =1.,

["pulprog"]= "inp4gl.pk",
o1 = spectrometer[now].proton_carrier_298k, 
o2p=spectrum.CaCb.carrier, 
o3p=spectrum.N.carrier,
o4p=spectrum.D.carrier,
["xcpr"]="parmode 2D",


 --set hard powers and pulses for f1-f4
 p1 = spectrometer[now].calibration_pulse90_f1,
pl1= spectrometer[now].calibration_power_db_f1,
p3  = spectrometer[now].calibration_pulse90_f2,
pl2= spectrometer[now].calibration_power_db_f2,
p5 = spectrometer[now].calibration_pulse90_f3,
pl3 =spectrometer[now].calibration_power_db_f3,
pcpd4 = pulses.Dcpd90, -- D decoupling
pl4 = calc_power_set(pulses.Dcpd90,1.0,spectrometer[now].calibration_pulse90_f4,spectrometer[now].calibration_power_db_f4),
["cpdprg4"]= "waltz16", 
pl19 =60, --water presat (when used) 
pcpd2 = pulses.Ccpd90,
pl12 = calc_power_set(pulses.Ccpd90,1.0,spectrometer[now].calibration_pulse90_f2,spectrometer[now].calibration_power_db_f2),
["cpdprg2"]="garp4",
pcpd3 = pulses.Ncpd90,
pl13 = calc_power_set(pulses.Ncpd90,1.0,spectrometer[now].calibration_pulse90_f3,spectrometer[now].calibration_power_db_f3),
["cpdprg3"]="waltz16",

--CO decoupling
 p12 =pulses.COdec90, -- it is not that critical, assume 300 is ideal for 700
 ["spnam12"] ="Gauss_5_256.pk", -- integral factor 0.5
 sp12 = calc_power_set(pulses.COdec90,0.5,spectrometer[now].calibration_pulse90_f2,spectrometer[now].calibration_power_db_f2),
 spoal12 =0.5, -- have to check what's that
 cnst21 = spectrum.CO.carrier,

--set increment times
 in0 = (0.5/(spectrum.CaCb.sw*spectrometer[now].B_field*IndirectChemicalShiftReferences.C13)),
 ["1 in_010 "]= (0.5/(spectrum.CaCb.sw*spectrometer[now].B_field*IndirectChemicalShiftReferences.C13)),
  --set number of sampled points
 ["1 td "]=(0.5/couplings.J_CC.Hz)/((0.5/(spectrum.CaCb.sw*spectrometer[now].B_field*IndirectChemicalShiftReferences.C13))), --C direction, limited by CC coupling, 8 ms is max Ca, signal goes to 0 in 1/(2*Jcacb)=13ms
 ["2 td "]=(1024), --H direction
 ["1 nd_010"]= 2 , --C direction 
 



 --gradients
["gpnam1"]="SINE.100",
["gpnam2"]=" SINE.100",
["gpnam3"]=" SINE.100",
["gpnam4"]=" SINE.100",
["gpnam5"]=" SINE.100",
["gpnam9"]=" SINE.100",
["gpnam10"]=" SINE.100",
 
gpz1 =19,
gpz2 =90,
gpz3 =85,
gpz4 =30,
gpz9  =90,
gpz10 =90, --plus or minus critical

p21= 800, --(Gradient in first INEPT)
p22= 1000, --(Gradient for 1st z-filter)
p23= 1000, --(Gradient for 2nd z-filter)
p24= 800, --(Gradient for 3rd z-filter)
p29= 995, --PFG 13C
p30= 250, --PFG 1H

--acqu modes
["1 FnMODE"]= "Echo-Antiecho",

--processing
["1 mc2"]= "echo-antiecho",
["2 absf1"]=  4.4, 
["2 absf2"]=  -1, 
["2 si"]=  2048,  --H dim
["2 stsi"]=  0,
["1 si"]=  512, -- n dim 
["1 phc0"]=  -36.05, --0, -- n dim
["1 phc1"]=  -112.8, ---180, -- n dim 
["2 ssb"]=  2, -- n dim
["1 ssb"]=  2, -- n dim 
["1 PH_mod"]=  "pk", -- n dim
["2 PH_mod"]=  "pk", -- n dim 
["1 REVERSE"]="TRUE",
["1 sf"]=spectrometer[now].B_field/(3.97) -- C dim 
}
</fld>
