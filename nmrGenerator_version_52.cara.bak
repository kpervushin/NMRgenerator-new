define list&lt;delay&gt; VCPMGList = { 4. 28. 20. 64. 16. 50. 8. 36.}
"d18=p1*1.33"

aqseq 312



1 ze
  2m 
2 20u pl19:f1
  20u do:f3
   d1 cw:f1
  20u do:f1 
   10u pl1:f1 pl2:f2 pl3:f3
3   (p1 ph20)
  d26 pl2:f2
  (center (p2 ph20) (p22 ph20):f3 )
  d26 UNBLKGRAD
  (p1 ph21) 
  4u pl0:f1
  (p11:sp1 ph20:r):f1
  4u
  p16:gp1
  d16 pl1:f1
"VCPMGList.idx = l10"

  (p21 ph3):f3

"d9=VCPMGList"
 
 "l12=d9"
 "d14=0.001"  ; -- 2 ms per CPMG cycle 
 
  
 p19:gp6
  d16
  d17
  (p11:sp1 ph12):f1
  (center (p1 ph21 2u p1*2 ph13 2u p1 ph21) (p21*2 ph20):f3 )
  (p11:sp1 ph12):f1
 p19:gp6
  d16 pl9:f1 ;--decrease 1H power
  d17 
  10u cpd1:f1
 12 d14
       (p21*2 ph20):f3
    d14
 lo to 12 times l12
10u do:f1
(p21 ph21):f3
p16:gp7
  d16 pl1:f1   ;--return 1H power 
 
(p21 ph17):f3

;----

d0
; 2.67m -1.5m (p16) - 1.5 (d16)
0.97m
 p16:gp2
  d16
(p2 ph20):f1 (p3*2 ph20):f2
d0
(p21*2 ph20):f3

 p16:gp21
  d16
0.97m


  
  (center (p1 ph22) (p21 ph4):f3 )
  p19:gp4
  d16
  DELTA2
  (center (p2 ph20) (p22 ph20):f3 )
  DELTA2
  p19:gp4
  d16
  (center (p1 ph21) (p21 ph21):f3 )
  p16:gp5
  d16
  DELTA3
  (center (p2 ph20) (p22 ph20):f3 )
  DELTA3
  p16:gp5
  d16
  (p1 ph20)
  DELTA1
  (p2 ph20)
  4u
  p16:gp3*EA
  d16 pl16:f3
  4u BLKGRAD
  go=2 ph31 cpd3:f3
  d1 do:f3 mc #0 to 2 
     F1QF(iu10)
     F2EA(ru10 &amp; igrad EA &amp; ip4*2, id0)
exit
   


