<fld name='Body' type='Memo'>;optimization of water flip back:
;- optimize watergate (o1,sp2,ph26)
;- optimize water flip back (sp1,ph16,ph18,ph17,ph19)

;K. Pervushin et al, PNAS, 94, 12366 (1997)

;pl1   : power for 1H
;pl2   : power for 13C
;pl3   : power for 15N

;sp1   : water flipback power
;spnam1: gauss128_5
 

;p1    : 90 degree hard pulse 1H
;p3    : 90 degree hard pulse 13C
;p4    : 180 degree hard pulse 13C (225d for 5/600)
;p5    : 90 degree hard pulse 15N
;p11   : water flipback pulse (1.5m)


;p21   : 500u (Gradient in first INEPT)
;p22   : 500u (Gradient in second INEPT)
;p23   : 900u (Gradient in watergate)
;gpz1  : 19%
;gpz2  : 15%
;gpz3  : 32%


;d1    : relaxation delay
;d2    : INEPT delay (~2.7ms)
;in0   : 1/(2 SW) (Hz)

define delay INEPT1
define delay INEPT2
define delay INEPT3
define delay INEPT4

#include &lt;Avance.incl&gt;

#define GRADIENT1   10u p21:gp1 200u
#define GRADIENT2   10u p22:gp2 200u
#define GRADIENT3   10u p23:gp3 200u
#define GRADIENT4   10u p24:gp4 200u

#define GR_DP 10u p29:gp9*plusminus 200u
#define GR_RP 10u p30:gp10 200u
define delay DP_COMP
"DP_COMP=p29+210u"
define delay RP_COMP
"RP_COMP=p30+210u"

"p2=2*p1"
"p6=2*p5"

"d0=in0/2-(p3*2 + 1.5u)"
"INEPT1=d2-(p21+210u)-10u"
"INEPT2=d2-(p22+p11+210u)-10u"
"INEPT4=d2-(p22+210u)-10u"
"INEPT3=d2-(p23+210u)-10u"

"l2 = 1"



1  10u ze
2  1m
   d1
   20u pl1:f1 
   20u pl2:f2
   20u pl3:f3 
   20u LOCKH_ON
;----------------------------------------first INEPT
5  10u 
   (p1 ph20):f1
   10u
   ;(p11:sp1 ph20):f1
   GRADIENT1
   INEPT1 pl1:f1
   (center(p2 ph21):f1 (p6 ph20):f3) 
   GRADIENT1
   INEPT1
   ;(p11:sp1 ph21):f1
   10u 
   (p1 ph23):f1
10u
(p11:sp1 ph22):f1
   10u
   GRADIENT4
;----------------------------------------15N evolution
   (p5 ph1):f3
   d0
   (p3 ph23 1.5u p4 ph20 1.5u p3 ph23):f2
   d0
    DP_COMP
   (p5*2 ph2):f3
    GR_DP
;----------------------------------------second INEPT
   (center(p1 ph20):f1 (p5 ph3):f3)
   10u
   (p11:sp1 ph17):f1
   GRADIENT2 
   INEPT2 pl1:f1
   (center(p2 ph20):f1 (p6 ph20):f3)
   GRADIENT2
   INEPT4
    ;(p11:sp1 ph16:r):f1 
   10u 
   (center(p1 ph21):f1 (p5 ph20):f3)
;----------------------------------------WATERGATE
   GRADIENT3
   INEPT3 
   ;(p11:sp1 ph26:r):f1
   10u 
   (center(p2 ph20):f1 (p6 ph20):f3)
   10u 
   ;(p11:sp1 ph26:r):f1  
   GRADIENT3
   INEPT3 LOCKH_OFF
   (center(p1 ph21):f1 (p5 ph21):f3)
   RP_COMP
   (p2 ph20):f1
   GR_RP
;----------------------------------------acquisition
   go=2 ph31 
   1m mc #0 to 2 F1EA(ip3*2 &amp; igrad plusminus,id0)
10u do:f1
10u do:f2
10u do:f3
10u LOCKH_OFF
exit

ph1 =0 2
ph2 =0 0 2 2
ph3 =0
ph31=0 2
ph17=0

ph20=0
ph21=1
ph22=2
ph23=3


