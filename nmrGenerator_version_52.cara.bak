<fld name='Body' type='Memo'>;hsqc13C.new
;dreier, November 2002

;carbonyl decoupling by shaped pulse with offset
;15N and 13C=O decoupling are not executed as long as d0 is too small

;G. Bodenhausen ans D.J. Ruben, Chem. Phys. Lett. 69, 185 (1980)
;D.R. Muhandiram et al, JMR B102, 317-321 (1993)

;pl1   : power for 1H 
;pl3   : power for 15N
;pl2   : power for 13C hard
;pl12  : power for 13C GARP decoupling
;sp12  : power for selective C=O pulse

;p1    : 90 degree hard pulse 1H
;p3    : 90 degree hard pulse 13C
;p4    : 13C pulse, 225deg for 500/600, 180deg for 750/900
;p5    : 90 degree hard pulse 15N
;pcpd2 : 90 deg cpd-pulse 13C (~85us)
;p12   : shaped 180 pulse for CO decoupling (80u)
;spnam12: shape for selective C=O pulse (gauss.128_5)

;p21   : 1m (Gradient in first INEPT)
;p22   : 800u (Gradient in first INEPT)
;p23   : 1m (Gradient for z-filter)
;p24   : 1m (Gradient for second INEPT)

;gpz1  : 19%
;gpz2  : 30%
;gpz3  : 65%
;gpz4  : 15%

;d1    : relaxation delay
;d2    : 1H-13C INEPT delay (1.7m)
;in0   : 1/2*SW(in Hz)

;cnst21: CO chemical shift (offset, in ppm)

#include &lt;Avance.incl&gt;

define delay INEPT_1
define delay INEPT_2

#define GRADIENT0  10u p20:gp0 200u
#define GRADIENT1  10u p21:gp1 200u
#define GRADIENT2  10u p22:gp2 200u
#define GRADIENT3  10u p23:gp3 200u
#define GRADIENT4  10u p24:gp4 200u

#define GR_DP 10u p29:gp9*plusminus 200u
#define GR_RP 10u p30:gp10 200u
define delay DP_COMP
"DP_COMP=p29+210u"
define delay RP_COMP
"RP_COMP=p30+210u"


"p2=p1*2"
"p6=p5*2"
"p4=p3*2"

"spoff12=bf2*(cnst21/1000000)-o2"

"d0=in0/2-p1/3.14"
"d10=in10/2-p1-p3*2/3.14-1u"
"INEPT_1=d2-(p21+210u)"
"INEPT_2=d2-(p24+210u)"

aqseq 312

1  10u ze
   10u
2  1m do:f2
   20u pl1:f1
   10u LOCKH_OFF
   d1 pl2:f2 
   20u pl3:f3
   20u LOCKH_ON
;--------------1H evolution
 
 
(p1 ph11:r d0 d0 p1 ph12):f1 (d0 p4 ph20):f2 (d0 p6 ph20):f3
;--------------mixing time
 ; GRADIENT1
  d8 
  GRADIENT0
;-----------------------------------------first INEPT
   (p1 ph20):f1
   GRADIENT1                
   INEPT_1                          
   (center (p2 ph20):f1 (p3 ph20 2u p4 ph21 2u p3 ph20):f2)
   GRADIENT1       
   INEPT_1
   (p1*300 ph20):f1                                                                  
   (p1 ph21):f1 
   GRADIENT2
;-----------------------------------------13C evolution
   (p3 ph2):f2
if "d10+p1&gt;p5" goto 7                     ;check whether there is enough time for 15N pulse
   (1u d10 p2 ph20 d10 1u):f1	                 ;no time for 15N pulse
 goto 9
7  1u
if "2*d10+p2&gt;p12+8u" goto 8                    ;is there also enough for selective 13C=O pulse?
   (refalign (d10 p2 ph20 d10 1u):f1 center (p6 ph20 1u):f3)                 ;no time for 13C pulse
 goto 9
8  (refalign (d10 p2 ph20 d10 1u):f1 center (p6 ph20 1u):f3 center (4u p12:sp12 ph20 4u 20u pl2):f2)
9  4u

   DP_COMP
   (p4 ph20):f2
   GR_DP

   (p3 ph1):f2
   GRADIENT3
   10u
   (p1 ph20):f1
;-----------------------------------------second INEPT
   GRADIENT4
   INEPT_2
   (center (p2 ph20):f1 (p4 ph20):f2)
   GRADIENT4
   INEPT_2 
   (p1 ph21):f1 (p3 ph21):f2
   893u 
  ;1786u
   (center (p2 ph20):f1 (p4 ph20):f2)
   893u
  ;1786u
   (p1 ph20):f1
   20u
   RP_COMP
   (p2 ph20):f1
   GR_RP


   20u pl12:f2 LOCKH_OFF
;-----------------------------------------acquisition
   go=2 ph31 cpd2:f2
   1m do:f2 mc #0 to 2 
   F1PH(ip12,id0)
   F2EA(rp12 &amp; rd0 &amp; ip1*2 &amp; igrad plusminus, id10)
   10u do:f1
   10u do:f2
   10u do:f3
   
   10u LOCKH_OFF
exit

ph2= 0 2
ph1= 0 0 
ph20=0
ph21=1
ph22=2
ph23=3
ph31=2 0 0 2
ph11 = 0 0 2 2
ph12 = 0 0 0 0
